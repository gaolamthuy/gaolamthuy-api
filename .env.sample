SUPABASE_URL=https://your-supabase-url.com
SUPABASE_SERVICE_KEY=your_supabase_service_key_here
KIOTVIET_BASE_URL=https://public.kiotapi.com
PORT=3001

# Authentication credentials (used for both media upload and KiotViet clone routes)
MEDIA_AUTH_USER=admin
MEDIA_AUTH_PASS=your_secure_password

# KiotViet webhook secret
KIOTVIET_WEBHOOK_SECRET=your_webhook_secret

# Cloudflare R2 / S3 credentials
S3_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
S3_ACCESS_KEY=your_access_key_here
S3_SECRET_KEY=your_secret_key_here
S3_BUCKET_NAME=your_bucket_name_here
S3_REGION=auto

# CDN endpoint for displaying images
CDN_ENDPOINT=https://your-cdn-url.com